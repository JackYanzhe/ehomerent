.swiper-container {
	width: 400px;
	height: 98px;
	padding: 16px 0 16px 0;
	border-right: 27px solid #fff;
	border-left: 27px solid #fff;
	box-sizing: border-box;
	overflow: visible;
} 
.swiper-slide{
	list-style-type: none;
	    background-size: cover;
	    background-position: center;
	    padding:0 3px;
	    background-clip: content-box;
	    box-sizing: border-box;
}
.p-img-wrap{
	width: 400px;
    height: 285px;
    overflow: hidden;
}
.p-img-wrap .p-img{
	width: 100%;
	height: 100%;
	background-position: center;
	position: relative;
	cursor: pointer;
    background-size: cover;
}
.swiper-button-prev {
    position: absolute;
    top: 50%;
    width: 27px;
    left: -27px;
    height: 66px;
    margin-top: -33px;
    z-index: 10;
    cursor: pointer;
    background-color: #ccc;
    -moz-background-size: 27px 66px;
    -webkit-background-size: 27px 66px;
    background-size: 27px 66px;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('../css/skins/simple/carouselLeftNav.png');
}
.swiper-button-next{
	position: absolute;
    top: 50%;
    width: 27px;
    right: -27px;
    height: 66px;
    margin-top: -33px;
    z-index: 10;
    cursor: pointer;
    background-color: #ccc;
    -moz-background-size: 27px 66px;
    -webkit-background-size: 27px 66px;
    background-size: 27px 66px;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('../css/skins/simple/carouselRightNav.png');
}
.swiper-button-next.swiper-button-disabled, .swiper-button-prev.swiper-button-disabled {
    opacity: .65;
    cursor: auto;
    pointer-events: none;
}



